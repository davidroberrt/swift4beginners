import UIKit

// Incrementos, operadores lógicos

let result = 2 <= 3

let x = 10
let y = 20
let res = x == y // igual
let res2 = x != y // diferente
 
let firstName = "David"
let lastName = "Robert"

firstName == lastName

// &&, || e !

let isDriver = false
let isStudent = true

// negação: !
!isDriver // inverter expressões, inversor de impressões


// ou: ||
let r = isDriver || isStudent
print(r)

// e: &&
let r2 = isDriver && isStudent
print(r2)

isDriver && 10 > 5

// + -

var z = 11
print(z+11)
var h = 12
print(h-12)
